    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleNameMapper": {
            "@config/(.*)": "<rootDir>/src/config/$1",
            "@logger": "<rootDir>/src/modules/shared/services/logger.service.ts",
            "@seeder/(.*)": "<rootDir>/src/modules/shared/modules/seeder/$1",
            "@cli/(.*)": "<rootDir>/src/modules/cli/$1",
            "@modules/(.*)": "<rootDir>/src/modules/$1",
            "@integrations/(.*)": "<rootDir>/src/integrations/$1",
            "@entities": "<rootDir>/src/modules/shared/entities/entities"
        },
        "rootDir": ".",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverage": false,
        "coverageDirectory": "./storage/coverage",
        "collectCoverageFrom": [
            "./src/**/*service.ts",
            "./src/**/*helper.ts",
            "./src/**/*integration.ts"
        ],
        "testEnvironment": "node"
    }
