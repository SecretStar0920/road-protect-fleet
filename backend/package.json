    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch --preserveWatchOutput",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node --max-old-space-size=8192 dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest --runInBand --verbose --forceExit",
        "test:watch": "gulp test:watch",
        "test:cov": "jest --coverage --verbose",
        "test:dev": "kill-port 9229; node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest $(npx entro-jest-flags ./test/custom-test-config.json) --watchAll --verbose --runInBand",
        "test:dev:pattern": "nodemon --watch=./test/custom-test-config.json --exec npm run test:dev",
        "test:debug": "kill-port 9229; node --inspect=0.0.0.0:9229 -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest $(npx entro-jest-flags ./test/custom-test-config.json) --verbose --watchAll",
        "test:e2e": "jest --config ./test/jest-e2e.json --verbose",
        "script-runner": "node",
        "docker-runner": "docker exec rp-dev_backend_1 sh -c",
        "console": "npm run script-runner -- dist/console.js",
        "docker-console": "npm run docker-runner -- \"npm run console\"",
        "cli": "npm run script-runner -- dist/cli/cli.js",
        "kill:debug-port": "kill-port 9229"
    },
